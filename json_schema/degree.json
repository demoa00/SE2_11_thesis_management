{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "degree_schema",
    "title": "Degree Schema",
    "type": "object",
    "properties": {
        "$schema": {
            "type": "string"
        },
        "degreeId": {
            "type": "string",
            "pattern": "^LM?-([1-9]|[1-9][0-9])$"
        },
        "titleDegree": {
            "type": "string",
            "minLength": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "degreeId",
        "titleDegree"
    ]
}