        "title": "Thesis Proposal",
        "version": "1.0.11"
    },
    "components": {
        "schemas": {
            "professor": {
                "type": "object",
                "properties": {
                    "professorId": {
                        "type": "string",
                        "pattern": "^(p|P)[0-9]{6}$"
                    },
                    "name": {
                        "type": "string",
                        "pattern": "^[a-zA-Z ]+$",
                        "minLength": 1
                    },
                    "surname": {
                        "type": "string",
                        "pattern": "^[a-zA-Z ]+$",
                        "minLength": 1
                    },
                    "email": {
                        "type": "string",
                        "format": "email",
                        "pattern": "^[a-z]+\\.[a-z]+@polito\\.it$"
                    },
                    "codGroup": {
                        "type": "string",
                        "pattern": "^CG_[0-9]{2}$"
                    },
                    "codDepartment": {
                        "type": "string",
                        "pattern": "^CD_[0-9]{2}$"
                    },
                    "password": {
                        "type": "string",
                        "minimum": 8,
                        "maximum": 20
                    },
                    "self": {
                        "type": "string",
                        "format": "uri"
                    }
                },
                "minProperties": 1,
                "maxProperties": 8,
                "required": [
                    "email"
                ]
            },
            "professors": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "professorId": {
                            "type": "string",
                            "pattern": "^(p|P)[0-9]{6}$"
                        },
                        "name": {
                            "type": "string",
                            "pattern": "^[a-zA-Z ]+$",
                            "minLength": 1
                        },
                        "surname": {
                            "type": "string",
                            "pattern": "^[a-zA-Z ]+$",
                            "minLength": 1
                        },
                        "self": {
                            "type": "string",
                            "format": "uri"
                        }
                    },
                    "minProperties": 4,
                    "maxProperties": 4
                }
            },
            "student": {
                "type": "object",
                "properties": {
                    "studentId": {
                        "type": "string",
                        "pattern": "^(s|S)[0-9]{6}$"
                    },
                    "name": {
                        "type": "string",
                        "pattern": "^[a-zA-Z ]+$",
                        "minLength": 1
                    },
                    "surname": {
                        "type": "string",
                        "pattern": "^[a-zA-Z ]+$",
                        "minLength": 1
                    },
                    "gender": {
                        "enum": [
                            "male",
                            "female",
                            "other"
                        ]
                    },
                    "email": {
                        "type": "string",
                        "format": "email",
                        "pattern": "^(s|S)[0-9]{6}@studenti\\.polito\\.it$"
                    },
                    "codDegree": {
                        "type": "string",
                        "pattern": "^LM?-([1-9]|[1-9][0-9])$"
                    },
                    "nationality": {
                        "type": "string",
                        "pattern": "^[a-zA-Z ]+$",
                        "minLength": 1
                    },
                    "enrollmentYear": {
                        "type": "integer"
                    },
                    "password": {
                        "type": "string",
                        "minimum": 8,
                        "maximum": 20
                    },
                    "self": {
                        "type": "string",
                        "format": "uri"
                    }
                },
                "minProperties": 1,
                "maxProperties": 10,
                "required": [
                    "email"
                ]
            },
            "application": {
                "type": "object",
                "properties": {
                    "thesisProposalId": {
                        "type": "integer"
                    },
                    "studentId": {
                        "type": "string",
                        "pattern": "^(s|S)[0-9]{6}$"
                    },
                    "message": {
                        "type": "string",
                        "maxLength": 1000
                    },
                    "date": {
                        "type": "string",
                        "format": "date"
                    },
                    "cv": {
                        "type": "string",
                        "format": "uri"
                    },
                    "isReaded": {
                        "type": "boolean",
                        "default": false
                    },
                    "isAccepted": {
                        "type": "boolean"
                    },
                    "self": {
                        "type": "string",
                        "format": "uri"
                    }
                },
                "minProperties": 2,
                "required": [
                    "thesisProposalId"
                ]
            },
            "applications": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "thesisProposalId": {
                            "type": "integer",
                            "minimum": 1
                        },
                        "thesisProposalTitle": {
                            "type": "string",
                            "minLength": 1
                        },
                        "date": {
                            "type": "string",
                            "format": "date"
                        },
                        "self": {
                            "type": "string",
                            "format": "uri"
                        }
                    },
                    "minProperties": 4,
                    "maxProperties": 4
                }
            },
            "thesisProposal": {
                "type": "object",
                "properties": {
                    "thesisProposalId": {
                        "type": "integer",
                        "minimum": 1
                    },
                    "title": {
                        "type": "string",
                        "minLength": 1
                    },
                    "supervisor": {
                        "type": "string",
                        "pattern": "^(p|P)[0-9]{6}$"
                    },
                    "coSupervisor": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "pattern": "^((p|P)|(e|E))[0-9]{6}$"
                        }
                    },
                    "keywords": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "minItems": 1
                    },
                    "thesisType": {
                        "type": "object",
                        "properties": {
                            "inCompany": {
                                "type": "boolean"
                            },
                            "abroad": {
                                "type": "boolean"
                            }
                        }
                    },
                    "groups": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "minLength": 1
                        },
                        "minItems": 1,
                        "uniqueItems": true
                    },
                    "description": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 1000
                    },
                    "requirements": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 1000
                    },
                    "notes": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 1000
                    },
                    "expirationDate": {
                        "type": "string",
                        "format": "date"
                    },
                    "level": {
                        "enum": [
                            "BSc",
                            "MSc"
                        ]
                    },
                    "CdS": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "minLength": 1
                        },
                        "minItems": 1
                    },
                    "isArchieved": {
                        "type": "boolean",
                        "default": false
                    },
                    "self": {
                        "type": "string",
                        "format": "uri"
                    }
                },
                "minProperties": 11,
                "maxProperties": 14,
                "required": [
                    "title",
                    "keywords",
                    "thesisType",
                    "description",
                    "requirements",
                    "expirationDate",
                    "level",
                    "CdS"
                ]
            },
            "thesisProposals": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "thesisProposalId": {
                            "type": "integer",
                            "minimum": 1
                        },
                        "title": {
                            "type": "string",
                            "minLength": 1
                        },
                        "keywords": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            },
                            "minItems": 1
                        },
                        "expirationDate": {
                            "type": "string",
                            "format": "date"
                        },
                        "self": {
                            "type": "string",
                            "format": "uri"
                        }
                    },
                    "minProperties": 5,
                    "maxProperties": 5
                }
            },
            "externalCoSupervisor": {
                "type": "object",
                "properties": {
                    "externalCoSupervisorId": {
                        "type": "string",
                        "pattern": "^(e|E)[0-9]{6}$"
                    },
                    "name": {
                        "type": "string",
                        "pattern": "^[a-zA-Z ]+$",
                        "minLength": 1
                    },
                    "surname": {
                        "type": "string",
                        "pattern": "^[a-zA-Z ]+$",
                        "minLength": 1
                    },
                    "email": {
                        "type": "string",
                        "format": "email"
                    },
                    "company": {
                        "type": "string",
                        "minLength": 1
                    },
                    "self": {
                        "type": "string",
                        "format": "uri"
                    }
                }
            }
